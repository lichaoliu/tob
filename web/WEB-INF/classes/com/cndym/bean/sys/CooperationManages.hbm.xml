<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cndym.bean.sys.CooperationManages" table="COOPERATION_MANAGES">
        <id name="id" column="ID">
			<generator class="sequence">
                <param name="sequence">SEQ_COOPERATION_MANAGES</param>
            </generator>
		</id>
        <property name="managesId" column="MANAGES_ID" type="java.lang.Long"/>
        <property name="cooperationId" column="COOPERATION_ID" type="java.lang.Long"/>
        <property name="createTime" column="CREATE_TIME" type="java.util.Date"/>
    </class>
</hibernate-mapping>